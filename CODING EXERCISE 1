package numericalData;

class CylinderComputation {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double radius = 5.6;
		double height = 7.5;
		double surfaceArea;
		double baseArea;
		double volume;
		final double PI = 3.14159265;
		
		surfaceArea = (2*PI*radius*radius)*(height*radius*2*PI);
		baseArea = (PI*radius*radius);
		volume = (radius*radius*height*PI);
		
		System.out.print("The total suface area is : ");
		System.out.println(surfaceArea);
		System.out.print("The base area is : ");
		System.out.println(baseArea);
		System.out.print("The volume is ");
		System.out.println(volume);
		
